# This is a Python program to demonstrate different data types

# Literal
x = "This can be any data type"
print("Literal:", x)

# Integer
y = 902787945650182997445959082887
print("Integer:", y)

# Float
z = 3.1415926535
print("Float:", z)

# String
name = "Euclid"
print("String:", name)

# Boolean
is_active = False
print("Boolean:", is_active)
